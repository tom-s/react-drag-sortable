{
  extends: "eslint:recommended",
  env: {
    browser: true,
    node: true,
    es6: true
  },
  parserOptions: {
    ecmaFeatures: {
      jsx: true
    }
  },
  plugins: [
    "react"
  ],
  rules: {
    react/jsx-uses-vars: 2,
    react/jsx-uses-react: 2,
    indent: [
      2,
      2,
      {
        SwitchCase: 1
      }
    ],
    semi: [
      2,
      "never"
    ],
    no-case-declarations: 0,
    eol-last: 2
  },
  globals: {
  },
  parser: "babel-eslint"
}
